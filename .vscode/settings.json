{
  "files.exclude": {
    "node_modules": true,
    "package-lock.json": false,
    ".nyc_output":true,
    "coverage": true
  },
  "codemetrics.basics.ComplexityLevelExtreme": 25,
  "codemetrics.basics.ComplexityLevelHigh": 20,
  "codemetrics.basics.ComplexityLevelNormal": 15,
  "codemetrics.basics.CodeLensHiddenUnder": 14,
  "linter_codeclimate.enabled": true,
  "files.associations": {
    "**/templates/**/*.ts": "plaintext",
    "**/templates/**/*.json": "plaintext"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": null
},
}

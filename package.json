{
  "name": "@gerard2p/mce",
  "version": "0.0.0-development",
  "description": "Make command executable easily",
  "main": "./index.js",
  "private": false,
  "bin": {
    "mce": "./mce"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "clean": "node incremental",
    "templates": "node cpx ./templates/**/*  lib/templates",
    "pkg-files": "node cpx \"?(package.json|README.md|LICENSE|mce)\"  lib",
    "compile": "tsc",
    "templates:w": "npm run templates -- -w",
    "pkg-files:w": "npm run pkg-files -- -w",
    "preclean": "rimraf ./incremental.tsbuildinfo",
    "compile:w": "tsc --incremental --tsBuildInfoFile ./incremental.tsbuildinfo -w",
    "build": "npm-run-parallel templates pkg-files compile",
    "dev": "npm-run-parallel clean templates:w pkg-files:w compile:w",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gerard2p/node-mce.git"
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/spinners.json",
    "/wcwidth/**/*.js",
    "/wcwidth/**/*.d.ts",
    "/commands/**/*.js",
    "/commands/**/*.d.ts",
    "/spinner/**/*.js",
    "/spinner/**/*.d.ts",
    "/core/**/*.js",
    "/core/**/*.d.ts",
    "/mce",
    "/README.md",
    "/LICENSE",
    "/templates/**/*"
  ],
  "keywords": [
    "mce",
    "commander",
    "command",
    "line",
    "cmd"
  ],
  "author": "Gerardo Pérez Pérez <gerard2perez@gerard2p.com.mx>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gerard2p/node-mce/issues"
  },
  "homepage": "https://github.com/gerard2p/node-mce#readme",
  "devDependencies": {
    "@types/cross-spawn": "^6.0.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.0.0",
    "chokidar": "^3.3.1",
    "glob": "^7.1.6",
    "jest": "^25.1.0",
    "npm-run-parallel": "^0.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "cross-spawn": "^7.0.1",
    "signal-exit": "^3.0.2"
  }
}
